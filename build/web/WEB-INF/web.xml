<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>compressMethod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compress</servlet-name>
        <jsp-file>/compress.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>decompress</servlet-name>
        <jsp-file>/decompress.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>compressMethod</servlet-name>
        <servlet-class>compressMethod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>decompressMethod</servlet-name>
        <servlet-class>decompressMethod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filemanage</servlet-name>
        <servlet-class>filemanage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>del</servlet-name>
        <servlet-class>del</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compress</servlet-name>
        <url-pattern>/compress</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>decompress</servlet-name>
        <url-pattern>/decompress</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compressMethod</servlet-name>
        <url-pattern>/compressMethod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>decompressMethod</servlet-name>
        <url-pattern>/decompressMethod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filemanage</servlet-name>
        <url-pattern>/filemanage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>del</servlet-name>
        <url-pattern>/del</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
</web-app>
